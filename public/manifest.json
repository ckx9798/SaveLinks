{
  "manifest_version": 3,
  "name": "SaveLinks",
  "version": "1.0",
  "action": {
    "default_popup": "/src/extension/popup.html",
    "default_icon": {
      "16": "link.svg"
    }
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
